{
  "name": "malmofoods",
  "version": "2.0.0",
  "description": "An app to find foods in Malm√∂. Hopefully.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -n server,client \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --trace-warnings index.js",
    "dev:client": "cd client && npm run dev",
    "build": "rm -rf dist/ && cd client && npm run build && cp -r dist ../",
    "start": "node index.js"
  },
  "author": "Peter Persson",
  "license": "ISC",
  "dependencies": {
    "cheerio": "1.0.0-rc.11",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.18.1",
    "express-useragent": "1.0.15",
    "moment": "2.29.3",
    "node-fetch": "3.2.4",
    "path": "0.12.7"
  },
  "devDependencies": {
    "concurrently": "7.2.1",
    "dotenv": "16.0.1",
    "eslint": "8.16.0",
    "nodemon": "2.0.16"
  },
  "nodemonConfig": {
    "ignore": [
      "files/*"
    ]
  }
}
