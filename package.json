{
  "name": "malmofoods",
  "version": "2.0.0",
  "description": "An app to find foods in Malm√∂. Hopefully.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -n tsserver,server,client \"npx tsc --watch\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon dist/index.js",
    "dev:client": "cd client && npm run dev",
    "build": "rm -rf dist/ && npx tsc && cp -r files/ dist/ && cd client && npm run build && cp -r dist ../dist/client",
    "start": "node dist/index.js"
  },
  "author": "Peter Persson",
  "license": "ISC",
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-useragent": "1.0.15",
    "moment": "2.29.4",
    "node-fetch": "3.2.10",
    "path": "0.12.7"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.31",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/express-useragent": "1.0.2",
    "@types/node": "18.8.3",
    "concurrently": "7.4.0",
    "dotenv": "16.0.3",
    "eslint": "8.25.0",
    "nodemon": "2.0.20",
    "typescript": "4.8.4"
  },
  "nodemonConfig": {
    "ignore": [
      "files/*"
    ]
  }
}
